  "description": "OCrafter Marketplace RESTFUL API",
  "main": "src/app.ts",
  "scripts": {
    "start": "nps",
    "test": "npm start test",
    "build": "npm start build",
    "presetup": "npm install",
    "generatedoc": "apidoc -i src -o src/public/apidoc",
    "setup": "npm start config && npm start setup.script",
    "dbseed": "npm start dbseed.script",
    "tslint": "tslint --project .",
    "add-migration": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "update-database": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "start-run": "cross-env NODE_ENV=production node dist/app.js",
    "prod": "npm run build && npm run start-run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tslint --project ."
    }
  },
  "keywords": [
    "NodeJS",
    "TypeScript",
    "express"
  ],
  "apidoc": {
    "name": "OCommerce-marketplace V1.0",
