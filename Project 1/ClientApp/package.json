{
  "name": "medcilia",
  "version": "6.3.0",
  "license": "https://themeforest.net/licenses/terms/regular",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --source-map=true --open --host 0.0.0.0 --port 4300  &REM ",
    "serve-prod": "ng serve --open --host 0.0.0.0 --prod --aot --port 4300  &REM ",
    "serve-stage": "ng serve --open  --configuration=staging --aot --port 4300  &REM ",
    "fake-server": "json-server --watch db.json --delay 10",
    "start": "npm run serve",
    "start-prod": "npm run serve-prod ",
    "start-stage": "npm run serve-stage ",
    "start-prod-deprecated": "concurrently \"npm run serve-prod\" \"npm run fake-server\"",
    "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
    "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
    "build": "node --max_old_space_size=8192  ./node_modules/@angular/cli/bin/ng build ",
    "post-build": "gulp purgecss",
    "build-stats": "node --max_old_space_size=8192  ./node_modules/@angular/cli/bin/ng build --stats-json",
    "build-prod": "node --max_old_space_size=8192  ./node_modules/@angular/cli/bin/ng build --prod --aot --source-map=false && npm run post-build && npm run ngsw:version",
    "build--configuration=staging": "node --max_old_space_size=8192  ./node_modules/@angular/cli/bin/ng build --configuration=staging --aot --source-map=true  && npm run post-build && npm run ngsw:version",
    "build-prod-stats": "node --max_old_space_size=8192  ./node_modules/@angular/cli/bin/ng build --prod --stats-json &&  npm run post-build && npm run ngsw:version",
    "analyze": "webpack-bundle-analyzer dist/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-watch": "concurrently --kill-others \"npm start\" \"npm run e2e-watch-no-serve\"",
    "e2e-watch-no-serve": "watch \"npm run e2e -- --dev-server-target=\" src e2e --wait=1",
    "ngsw:version": "ngsw-config dist ngsw-config.json",
    "graphql": "npm run graphql:clean && npm run graphql:get-schema && npm run graphql:queries && npm run graphql:generate  && npm run graphql:fragments",
    "graphql:clean": "rimraf src/app/blocks/graphql/generated/*",
    "graphql:get-schema": "get-graphql-schema http://localhost:24550/graphql > schema.graphql",
    "graphql:generate": "graphql-codegen --config codegen.yml",
    "graphql:introspect": "node_modules/.bin/gql-gen --schema http://localhost:24550/graphql --template node_modules/graphql-codegen-introspection-template --out schema.json",
    "graphql:queries": "node_modules/.bin/gqlg --schemaFilePath schema.graphql --destDirPath ./src/app/blocks/graphql/generated",
    "graphql:bases": "gql-gen --schema schema.graphql --template ./src/assets/development/gql-ts-classes-template  --out ./src/app/blocks/graphql/generated/bases.ts ./src/app/blocks/graphql/generated/**/*.gql",
    "graphql:connection-editor": "ts-node ./src/assets/development/gql-connection.ts",
    "graphql:fragments:DEPRECATED": "graphql generate-fragments -p default -o src/app/blocks/graphql/generated -g js",
    "graphql:fragments": "gqlgfrag --schemaFilePath schema.graphql --destDirPath src/app/blocks/graphql/generated",
    "remove_evil:apollo-link-error-modules": "rimraf node_modules/apollo-link-error/node_modules",
    "remove_evil:apollo-link-dedup-modules": "rimraf node_modules/apollo-link-dedup/node_modules",
    "remove_evil:typings": "run-s remove_evil:apollo-link-error-modules remove_evil:apollo-link-dedup-modules ",
    "postinstall": "run-s remove_evil:typings"
  },
